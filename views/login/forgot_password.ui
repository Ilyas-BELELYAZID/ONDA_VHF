   <string notr="true">/* --- Global Style --- */
QMainWindow {
	background-color: #f3f4f6; /* Light gray background */
	font-family: Inter, sans-serif;
}

/* --- Main Frame Style --- */
QFrame#resetFrame {
	background-color: white;
	border: 1px solid #e5e7eb;
	border-radius: 12px;
}

/* --- Title and Labels Style --- */
QLabel#titleLabel {
    font-size: 20px;
    font-weight: bold;
    color: #1f2937;
    qproperty-alignment: 'AlignCenter';
}

QLabel {
	font-size: 14px;
	font-weight: bold;
	color: #1f2937;
}

/* --- Input Field Style --- */
QLineEdit {
	padding: 12px;
	border: 1px solid #d1d5db;
	border-radius: 8px;
	font-size: 14px;
	background-color: #ffffff;
}

QLineEdit:focus {
	border: 2px solid #2563eb; /* Blue border on focus */
}

/* --- Button Style --- */
QPushButton {
	background-color: #2563eb; /* Blue */
	color: white;
	padding: 12px;
	border: none;
	border-radius: 8px;
	font-size: 14px;
	font-weight: bold;
}

QPushButton:hover {
	background-color: #1d4ed8; /* Darker blue on hover */
}

/* --- 'Back to Login' Button Style --- */
QPushButton#backToLoginButton {
	border: none;
	font-size: 13px;
	background-color: #FFFFFF;
	color: #6b7281; /* Gray */
    qproperty-alignment: 'AlignCenter';
    font-weight: normal; /* Override bold */
}

QPushButton#backToLoginButton:hover {
	color: #1d4ed8; /* Blue on hover */
}

/* --- 'ONDA' Logo Text Style --- */
QLabel#logoText {
	font-size: 28px;
	font-weight: 800; /* Extra bold */
	color: #1e3a8a; /* Dark blue */
}

/* Make the stacked widget background transparent */
QStackedWidget, QStackedWidget &gt; QWidget {
    background-color: transparent;
}</string>
